<simulation>
  <control>
    <duration>10</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>

  <archetypes>
    <spec><path>shorttermstorage</path><lib>ShorttermstorageFacility</lib><name>ShorttermstorageFacility</name></spec>
    <spec><path>shorttermstorage</path><lib>ShorttermstorageRegion</lib><name>ShorttermstorageRegion</name></spec>
    <spec><path>shorttermstorage</path><lib>ShorttermstorageInst</lib><name>ShorttermstorageInst</name></spec>
  </archetypes>

  <facility>
    <name>OneFacility</name>
    <config>
      <ShorttermstorageFacility />
    </config>
  </facility>

  <region>
    <name>OneRegion</name>
    <config>
      <ShorttermstorageRegion />
    </config>
    <institution>
      <name>OneInst</name>
      <initialfacilitylist>
        <entry>
          <prototype>OneFacility</prototype>
          <number>1</number>
        </entry>
      </initialfacilitylist>
      <config>
        <ShorttermstorageInst />
      </config>
    </institution>
  </region>

</simulation>
